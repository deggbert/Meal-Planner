{
  "name": "meal-planner",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --watch",
    "build prod": "ng build --watch --prod",
    "emulate": "firebase emulators:start --import dbInit",
    "export db": "firebase emulators:export dbInit --force",
    "kill emulator": "kill-port 8080",
    "t": "concurrently -n \"ngbuild,emulators\" \"ng build --watch\" \"firebase emulators:start --import dbInit\""
